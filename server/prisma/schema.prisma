generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Appointment {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String
  type      AppointmentType
  time      DateTime
  notes     String?
  createdAt DateTime @default(now())
}

enum AppointmentType {
  MANICURE
  PEDICURE
  BOTH
}
